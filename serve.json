{
  "headers": [
    {
      "source" : "**/*.*",
      "headers" : [{
        "key" : "Access-Control-Allow-Origin",
        "value" : "*"
      },
      {
        "key" : "Service-Worker-Allowed",
        "value" : "/"
      }]
    },
    
    {
      "source" : "**/*.@(jpg|jpeg|gif|png)",
      "headers" : [{
        "key" : "Cache-Control",
        "value" : "max-age=7200"
      }]
    },
    {
      "source" : "404.html",
      "headers" : [{
        "key" : "Cache-Control",
        "value" : "max-age=300"
      }]
    }
  ]
}
